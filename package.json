{
  "name": "trealla",
  "version": "0.13.2",
  "description": "Trealla Prolog bindings for JS",
  "main": "trealla.js",
  "type": "module",
  "types": "index.d.ts",
  "files": [
    "ATTRIBUTION",
    "trealla.js.map"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guregu/trealla-js.git"
  },
  "scripts": {
    "clean": "rm -f src/libtpl.wasm trealla.js trealla.js.map",
    "update": "git submodule update --init --recursive",
    "compile": "cd trealla && make clean libtpl && cp libtpl.wasm ..",
    "build": "node make.cjs"
  },
  "keywords": [
    "prolog",
    "logic",
    "logic-programming",
    "interpreter",
    "trealla-prolog"
  ],
  "author": "guregu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guregu/trealla-js/issues"
  },
  "homepage": "https://github.com/guregu/trealla-js#readme",
  "dependencies": {
    "@wasmer/wasi": "^1.1.2"
  },
  "devDependencies": {
    "esbuild": "^0.15.10",
    "node-stdlib-browser": "^1.2.0"
  }
}
